package chap1;

public class java2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean bVar = true;
		int nVar = 12;
		float fVar = 3.14f;
		char cVar = 'J';

		System.out.println("bool형 : " + bVar + " 정수형 : " + nVar);
		System.out.println("부동소수점형 : " + fVar + " 문자상수 : " + cVar);
	}

}
